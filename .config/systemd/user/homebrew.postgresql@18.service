[Unit]
Description=Homebrew generated unit for postgresql@18

[Install]
WantedBy=default.target

[Service]
Type=simple
ExecStart="/home/linuxbrew/.linuxbrew/opt/postgresql@18/bin/postgres" "-D" "/home/linuxbrew/.linuxbrew/var/postgresql@18"
Restart=on-failure
WorkingDirectory=/home/linuxbrew/.linuxbrew
StandardOutput=append:/home/linuxbrew/.linuxbrew/var/log/postgresql@18.log
StandardError=append:/home/linuxbrew/.linuxbrew/var/log/postgresql@18.log
Environment="LC_ALL=en_US.UTF-8"
